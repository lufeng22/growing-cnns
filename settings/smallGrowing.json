{
    "growing": true,

    "initialChannels": 8,
    "numSections": 2,
    "initialNumNodes": 2,

    "growthSteps": 3,
    "growthMode": "expandEdge",
    "numConvToAdd": 1,
    "itemsToExpand": "youngest",
    "copyBatchNorm": true,
    "randomWeights": false,
    "join": {
	    "type": "softmax",
    	    "preserve": 0.9
    },
    "stopping": {
	    "type": "fixedEpochs",
	    "numEpochs": 1
    },

    "batchSize": [512, 512, 512],
    "initialLearningRate": 0.01,
    "momentum": 0.9,
    "weightDecay": 1e-4,
    "batchNorm": true,

    "lrDecayEpochStep": 1,
    "lrDecayEpochRatio": 1.0,
    "lrDecayGrowthRatio": 1.0
}
